function largestOfFour (arr) {
  var result = [];
  for (var n in arr) {
    var maxNumber = 0;
    for (var s in arr[n]) {
      if (arr[n][s] > maxNumber) {
        maxNumber = arr[n][s];
      }
    }
    result[n] = maxNumber;
  }
  console.log(result);
}

largestOfFour([[4, 5, 1, 3], [13, 27, 18, 26], [32, 35, 37, 39], [1000, 1001, 857, 1]]);  //[5,27,39,1001]
